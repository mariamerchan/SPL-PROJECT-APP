{
  "name": "spl-project-app",
  "version": "1.0.0",
  "description": "SPL - Aplicación Monolítica",
  "main": "index.js",
  "scripts": {
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && npm install",
    "build-frontend": "cd frontend && npm run build",
    "build": "npm-run-all install-frontend install-backend build-frontend",
    "start-backend": "cd backend && node index.js",
    "start-frontend": "cd frontend && npm run start",
    "start": "npm-run-all --parallel start-backend start-frontend"
  },
  "keywords": [],
  "author": "Lihtsus Agile: construyendo mejores organizaciones",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
